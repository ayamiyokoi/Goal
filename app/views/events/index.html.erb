<p id="notice"><%= notice %></p>
<div class="container">
  <div class="row">
    <div class="col">


<h1>Events</h1>
<div id="calendar"></div>
<table>
  <thead>
    <tr>

      <th>Title</th>
      <th>Body</th>
      <th>Start</th>
      <th>End</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @events.each do |event| %>
      <tr>

        <td><%= event.title %></td>
        <td><%= event.body %></td>
        <td><%= event.start %></td>
        <td><%= event.end %></td>
        <td><%= link_to 'Show', event %></td>
        <td><%= link_to 'Edit', edit_event_path(event) %></td>
        <td><%= link_to 'Destroy', event, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
<div>
<%= link_to 'New Event', new_event_path %>
</div>
 </div>
  </div>
</div>
